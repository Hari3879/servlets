<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ServletControler</servlet-name>
  	<display-name>ServletControler</display-name>
  	<description></description>
  	<servlet-class>com.ojas.controler.ServletControler</servlet-class>
  </servlet>
  <servlet-mapping>
